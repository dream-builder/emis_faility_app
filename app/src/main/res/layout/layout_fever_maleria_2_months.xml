    <!--Symptom 85 Segment-->
    <RelativeLayout
        android:id="@+id/childcaresymptom85Segement"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.v7.widget.AppCompatCheckBox
            android:id="@+id/childcaresymptom85checkbox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:clickable="false"
            android:enabled="false"
            android:text="@string/childcaresymptom81"
            android:textColor="@color/black"
            android:textSize="18sp"/>

        <!--<TextView
            android:id="@+id/childcaresymptom85TextviewLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/childcaresymptom81"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:layout_toRightOf="@id/childcaresymptom85checkbox"
            android:layout_marginTop="10dp"/>-->

    </RelativeLayout>

