<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/fragmentborder">

    <TextView android:id="@+id/pregSearchSlTextView"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:padding="1dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="#000000"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchElcoTextView"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:padding="2dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="#000000"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchVillageTextView"
        android:layout_width="0dp"
        android:layout_weight="3.5"
        android:padding="2dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="#000000"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchDetailTextView"
        android:layout_width="0dp"
        android:layout_weight="6"
        android:padding="2dp"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:textColor="#000000"/>

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchAgeTextView"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:padding="5dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="#000000" />

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchLccTextView"
        android:layout_width="0dp"
        android:layout_weight="2"
        android:padding="2dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="#000000" />

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchGravidaTextView"
        android:layout_width="0dp"
        android:layout_weight="1.5"
        android:padding="2dp"
        android:layout_height="match_parent"
        android:gravity="center"
        android:textColor="#000000" />

    <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchLmpTextView"
        android:layout_width="0dp"
        android:layout_weight="3.5"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:textColor="#000000" />

   <View
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:background="#FAAC58"/>

    <TextView android:id="@+id/pregSearchEddTextView"
        android:layout_width="0dp"
        android:layout_weight="3.5"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:textColor="#000000" />

</LinearLayout>